import Popup from "./Popup.js";
export default class PopupDeleteCards extends Popup {
    constructor({popupSelector, formSubmitter}) {
        super(popupSelector);
        this._formSubmitter = formSubmitter
        
    }

    setSubmit(newCard) {
        super.open();
        this._newCard = newCard;
    }


    // close() {
    //     super.close();
    //   }
      
    setEventListeners() {
        super.setEventListeners();
        this._popup.addEventListener('submit', (evt) => {
            evt.preventDefault();
            this._formSubmitter(this._newCard);
        });
    };

}